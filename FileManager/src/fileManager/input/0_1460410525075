<script type="text/javascript" language="javascript">
	// <![CDATA[
		function requestValidateSiteFeedback(optionalStrTranslated){
			$('ajaxErrorsDiv').hide();
		
			var isValid = true;
			var formInputs = $$('#feedback textarea.field-required, #feedback select.field-required, #feedback input.field-required');
			formInputs.each(function (element) {
				if(element.up('div').hasClassName('indicate-error'))element.up('div').removeClassName('indicate-error');
				if(!$F(element)){
					element.up('div').addClassName('indicate-error');
					// $('form-error-report').show();
					isValid = false;
				}
			});
		
			if (isValid) {
			
				//  Remove the word optional from any input that is optional and has that value.
				var optionalInputs = $$('#feedback input.optional');
				optionalInputs.each(function (element) {
					if (element.value == optionalStrTranslated) {
						element.value = '';
					}
				});
				
				new Ajax.Updater({success:'ajaxErrorsDiv', failure:'ajaxErrorsDiv'}, 'sendSiteFeedback.do', { parameters: $('feedback').serialize() + '&errorForward=ajax.failure&forward=ajax', evalScripts:true,
					onSuccess:function(){
						$('feedback-form').hide();
					},
					onFailure:function() {
						$('ajaxErrorsDiv').show();
					}
				});
			}
			
			return isValid;
		}
			// ]]>
</script>
